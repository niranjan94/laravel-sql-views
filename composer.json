  "homepage": "https://github.com/niranjan94/laravel-sql-views",
  "version": "1.0.0",
  "license": "MIT",
  "authors": [
    {
      "name": "Niranjan Rajendran",
      "email": "niranjan94@yahoo.com"
    }
